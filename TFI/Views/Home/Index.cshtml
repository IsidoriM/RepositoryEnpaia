                    })
                })
            </script>
            <script>
                if (localStorage.getItem('id') == "btnMostraTFR") {
                    $("#rowTFR").show();
                    $("#rowFondo").hide();
                    $("#rowInfortuni").hide();

                }
                else if (localStorage.getItem('id') == "btnMostraFondo") {
                    $("#rowTFR").hide();
                    $("#rowFondo").show();
                    $("#rowInfortuni").hide();
                }
                else if (localStorage.getItem('id') == "btnMostraInfortuni") {
                    $("#rowTFR").hide();
                    $("#rowFondo").hide();
                    $("#rowInfortuni").show();
                }
                else {
                    $("#rowTFR").hide();
                    $("#rowFondo").hide();
                    $("#rowInfortuni").hide();
                }
            </script>
            <script>
                $("#btnMostraTFR2").click(function () {

                    $("#rowTFR").show();
                    $("#rowFondo").hide();
                    $("#rowInfortuni").hide();
                });
                $("#btnMostraFondo2").click(function () {
                    $("#rowTFR").hide();
                    $("#rowFondo").show();
                    $("#rowInfortuni").hide();
                });
                $("#btnMostraInfortuni2").click(function () {
                    $("#rowTFR").hide();
                    $("#rowFondo").hide();
                    $("#rowInfortuni").show();
                });
            </script>
        }
