                        labels: [
                            'Lavorativi',
                            'Extra lavorativi',
                            'In itinere',

                        ],
                        datasets: [
                            {
                                data: [3, 4, 5],
                                backgroundColor: ['aqua', 'black', 'pink'],
                            }
                        ]
                    }
