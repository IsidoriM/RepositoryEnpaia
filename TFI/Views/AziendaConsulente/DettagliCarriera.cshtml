
@model TFI.OCM.AziendaConsulente.RapportiLavoro

@{
    ViewBag.Title = "Dettaglio Carriera";
    Layout = "~/Views/Shared/_LayoutContainer.cshtml";
}
@section showMessage{
    @{Html.RenderPartial("PartialViewShowMessages");}

}
<style>
    .table td {
        text-align: center;
        vertical-align: middle;
    }

    .table th {
        background: #0c773c;
        color: white;
        width: fit-content;
        text-align: center;
    }

    .btn:hover {
        background-color: #0c773c
    }
</style>

@{
    var dettagli = Model.detCar;
}

<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header" style="background: #0c773c;text-align: center;width: 100%;color: white;font-size: 25px">
                        Visualizzazione Dettaglio Carriera Matricola: @dettagli.matricola
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="card card-success card-tabs">
                                    <div class="card-header p-0 pt-1" style="background-color: #0c773c;">
                                        <ul class="nav nav-tabs" id="custom-tabs-one-tab" role="tablist">
                                            <li class="nav-item col-sm-3">
                                                <a class="nav-link not-interactive" id="datiAnagraficiTab" data-toggle="pill" href="#custom-tabs-one-home" role="tab" aria-controls="custom-tabs-one-home" aria-selected="true">Dati Anagrafici</a>
                                            </li>
                                            <li class="nav-item col-sm-3">
                                                <a class="nav-link not-interactive active" id="datiContrattualiTab" data-toggle="pill" href="#custom-tabs-one-profile" role="tab" aria-controls="custom-tabs-one-profile" aria-selected="false">Dati Contrattuali</a>
                                            </li>
                                            <li class="nav-item col-sm-3">
                                                <a class="nav-link not-interactive" id="datiRetributiviTab" data-toggle="pill" href="#custom-tabs-one-messages" role="tab" aria-controls="custom-tabs-one-messages" aria-selected="false">Dati Retributivi</a>
                                            </li>
                                        @if (ViewBag.IsAziSomm)
                                        {
                                            <li class="nav-item col-sm-3">
                                                <a class="nav-link not-interactive" id="aziendaUtilizzatriceTab" data-toggle="pill" href="#custom-tabs-one-settings" role="tab" aria-controls="custom-tabs-one-settings" aria-selected="false">Azienda utilizzatrice</a>
                                            </li>
                                        }
                                        </ul>
                                    </div>
                                    <div class="card-body">
                                        <div class="tab-content" id="custom-tabs-one-tabContent">
                                            <div class="tab-pane fade" id="custom-tabs-one-home" role="tabpanel" aria-labelledby="datiAnagraficiTab">
                                                <form id="formModAnag" autocomplete="off" method="post">
                                                    <div class="row">
                                                        <div class="col-sm-4">
                                                            <div class="form-group">
                                                                <label>Matricola</label>
                                                                <input type="text" class="form-control" value="@dettagli.matricola" disabled>
                                                                <input type="text" class="form-control" id="prot" value="@dettagli.prot" hidden>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <div class="form-group">
                                                                <label>Cognome</label>
                                                                <input type="text" class="form-control" value="@dettagli.cognome" disabled>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <div class="form-group">
                                                                <label>Nome</label>
                                                                <input type="text" class="form-control" value="@dettagli.nome" disabled>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-sm-4">
                                                            <div class="form-group">
                                                                <label>Codice Fiscale</label>
                                                                <input type="text" class="form-control" value="@dettagli.codFis" disabled>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <div class="form-group">
                                                                <label>Data Nascita</label>
