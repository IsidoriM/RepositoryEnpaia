                preventivoRistrutturazione.className = "col-4 form-group";
                var lblPreventivoRistrutturazione = "<label class='control-label'>Preventivo Ristrutturazione</label>";
                var filePreventivoRistrutturazione = "<input class='form-control-file documento-file' name='preventivoRistrutturazioneFile' type='file' accept='.pdf, .jpeg, .jpg, .png' required />";
                preventivoRistrutturazione.innerHTML = lblPreventivoRistrutturazione + filePreventivoRistrutturazione;
                $('#DocumentazioneDaAllegare').append(preventivoRistrutturazione);

                var fatturaRistrutturazione = document.createElement('div');
                fatturaRistrutturazione.setAttribute('id', 'fatturaRistrutturazione');
                fatturaRistrutturazione.className = "col-4 form-group";
                var lblFatturaRistrutturazione = "<label class='control-label'>Fattura Ristrutturazione</label>";
                var fileFatturaRistrutturazione = "<input class='form-control-file documento-file' name='FatturaRistrutturazioneFile' type='file' accept='.pdf, .jpeg, .jpg, .png' required />";
                fatturaRistrutturazione.innerHTML = lblFatturaRistrutturazione + fileFatturaRistrutturazione;
                $('#DocumentazioneDaAllegare').append(fatturaRistrutturazione);

                break;
            default:
                $('#DocAll').hide();
                $('#DocumentazioneDaAllegare').empty();

                break;
        }
    });

</script>
