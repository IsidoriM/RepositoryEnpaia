
@{
    ViewBag.Title = "Anagrafica";
    Layout = "~/Views/Shared/_LayoutContainer.cshtml";
}

<style>
    .btn:hover {
        background-color: #0c773c;
    }
</style>

<div class="card card-default">
    <div style="background-color: #0c773c" class="card-header">
        <h3 style="color:white;text-align:center;width:100%;font-size:25px" class="card-title">Recapiti di @ViewBag.NomeUtente</h3>
    </div>
    <!---->
    <!-- /.card-header -->
    <div class="card-body">
        <div class="row">
            <div class="col-sm-2">
                <div class="form-group">
                    <label>Matricola</label>
                    <input type="text" class="form-control" value="0123456" asp-for="AnagMat" name="AnagMat" id="AnagMat" placeholder="Inserire Matricola" disabled>
                </div>
            </div>
            <div class="col-sm-5">
                <div class="form-group">
                    <label>Cognome</label>
                    <input type="text" class="form-control" value="Rossi" placeholder="Inserire Cognome" disabled>
                </div>
            </div>
            <div class="col-sm-5">
                <div class="form-group">
                    <label>Nome</label>
                    <input type="text" class="form-control" value="Mario" placeholder="Inserire Nome" disabled>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-2">
                <div class="form-group">
                    <label>Codice Fiscale</label>
                    <input type="text" class="form-control" value="NSTNCL58R23H3401" placeholder="Inserire Codice Fiscale" disabled>
                </div>
            </div>
            <div class="col-sm-5">
                <div class="form-group">
                    <label>Data Nascita</label>
                    <div class="input-group date" id="reservationdate" data-target-input="nearest">
                        <input type="text" class="form-control datetimepicker-input" value="02/03/1946" data-target="#reservationdate" disabled>
                        <div class="input-group-append" data-target="#reservationdate" data-toggle="datetimepicker">
                            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-5">
                <div class="form-group">
                    <label>Sesso</label>
                    <select class="form-control" disabled>
                        <option selected>M</option>
                        <option>F</option>

                    </select>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-2">
                <div class="form-group">
                    <label>Comune</label>
                    <input type="text" class="form-control" value="Roma" placeholder="Inserire Comune" disabled>
                </div>
            </div>
            <div class="col-sm-5">
                <div class="form-group">
                    <label>Provincia</label>
                    <input type="text" class="form-control" value="RM" placeholder="Inserire Provincia" disabled>
                </div>
            </div>
            <div class="col-sm-5">
                <div class="form-group">
                    <label>Stato Estero</label>
                    <input type="text" class="form-control" value="" placeholder="Inserire Stato Estero" disabled>
                </div>
            </div>
        </div>
        <hr />
        <div class="row">
            <div class="col-sm-2">
                <div class="form-group">
                    <label>Titolo di Studio</label>
                    <select class="form-control">
                        @{
                            foreach (var ts in ViewBag.TitoloStudio)
                            {
                                if (ts.dentistu == "DIPLOMA")
                                {
                                    <option selected value="@ts.dentistu">@ts.dentistu</option>
                                }
                                else
                                {
                                    <option value="@ts.dentistu">@ts.dentistu</option>
                                }

                            }
                        }
                        <!-- <option>LICENZA ELEMENTARE</option>
     <option>LICENZA MEDIA</option>
     <option>LAUREA</option>
     <option selected>DIPLOMA</option>-->
                    </select>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-2">
                <div class="form-group">
                    <label>Indirizzo di Residenza</label>
                    <select class="form-control">
                        <option selected>Via</option>
                        <option>Viale</option>
                        <option>Piazza</option>
                    </select>
                </div>
            </div>
            <div class="col-sm-5">
                <div class="form-group">
                    <label>&nbsp;</label>
                    <input type="text" class="form-control" value="Milano" placeholder="Inserire indirizzo">
                </div>
            </div>
            <div class="col-sm-5">
                <div class="form-group">
                    <label>Civico</label>
                    <input type="text" class="form-control" value="1" placeholder="Inserire il civico">
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-2">
                <div class="form-group">
                    <label>Comune</label>
                    <input type="text" class="form-control" value="Roma" placeholder="Inserire Comune">
                </div>
            </div>
            <div class="col-sm-5">
                <div class="form-group">
                    <label>Cap</label>
                    <input type="text" class="form-control" value="00132" placeholder="Inserire Cap">
                </div>
            </div>
            <div class="col-sm-5">
                <div class="form-group">
                    <label>Provincia</label>
                    <input type="text" class="form-control" value="RM" placeholder="Inserire la Provincia">
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-2">
                <div class="form-group">
                    <label>Località</label>
                    <input type="text" class="form-control" value="Roma" placeholder="Inserire la Località">
                </div>
            </div>
            <div class="col-sm-5">
                <div class="form-group">
                    <label>Stato Estero</label>
                    <input type="text" class="form-control" value="" placeholder="Inserire lo Stato Estero">
                </div>
            </div>
            <div class="col-sm-5">
                <div class="form-group">
                    <label>C/O</label>
                    <input type="text" class="form-control" value="" placeholder="">
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-2">
                <div class="form-group">
                    <label>Telefono</label>
                    <input type="tel" class="form-control" value="062234295" placeholder="Inserire Telefono" pattern="[0-9]{2}[0-9]{7}">
                </div>
            </div>
            <!--<div class="col-sm-5">
                <div class="form-group">
                    <label>Telefono 2</label>
                    <input type="text" class="form-control" value="" placeholder="Inserire Telefono2">
                </div>
            </div>-->
            <div class="col-sm-5">
                <div class="form-group">
                    <label>Cellulare</label>
                    <input type="tel" class="form-control" value="3477480756" placeholder="Inserire il Cellulare" pattern="[0-9]{3}[0-9]{7}">
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-2">
                <div class="form-group">
                    <label>Email</label>
                    <input type="text" class="form-control" value="mario.rossi@gmail.com" placeholder="Inserire Email">
                </div>
            </div>
            <div class="col-sm-5">
                <div class="form-group">
                    <label>Posta Elettronica Certificata</label>
                    <input type="text" class="form-control" value="" placeholder="Inserire la PEC">
                </div>
            </div>
           <!--<div class="col-sm-5">
                <div class="form-group">
                    <label>Fax</label>
                    <input type="text" class="form-control" value="" placeholder="Inserire il Fax">
                </div>
            </div>-->
            <div class="col-sm-5">
                <div class="form-group">
                    <label>Iban</label>
                    <input type="text" class="form-control" value="" placeholder="Inserire il Iban">
                </div>
            </div>
            <div class="col-sm-5">
                <div class="form-group">
                    <label>Codice Bic/Swift</label>
                    <input type="text" class="form-control" value="" placeholder="Inserire il Codice Bic/Swift">
                </div>
            </div>
        </div>
        <br />
        <div class="row justify-content-end">
            <div class="float-left col-sm-2">
                <button type="button" onclick="history.go(-1)" class="btn btn-block btn-outline-success btn-lg">Indietro</button>
            </div>
            <div class="col-sm-2">
                <button type="button" class="btn btn-block btn-outline-success btn-lg">Modifica</button>
            </div>
        </div>

    </div>
</div>

