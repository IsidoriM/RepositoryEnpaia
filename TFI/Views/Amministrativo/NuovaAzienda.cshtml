@section JS{
	<script>
		$(document).ready(function () {
			$(document).on("change", "#CategoriaAttivita", function (evt) {
				var select = $(evt.target);

				var current = $(select).find("option:selected").val();

				$.each($("#TipoAttivita").children("option"), function (idx, opt) {
					var option = $(opt);

					if (option.data("cat-att") == current) {
						option.removeClass("d-none");
					} else if (!option.hasClass("d-none")) {
						option.addClass("d-none");
					}
				});
			});

			$(document).on("change", "#Comune", function (evt) {
				var select = $(evt.target);

				var current = $(select).find("option:selected").val();

				$.each(select.parents(".tab-pane").find("#Localita").children("option"), function (idx, opt) {
					var option = $(opt);

					if (option.data("cod-com") == current) {
						option.removeClass("d-none");
					} else if (!option.hasClass("d-none")) {
						option.addClass("d-none");
					}
				});
			});

			$("#CategoriaAttivita").change();
			$("#Comune").change();
		})
	</script>
}