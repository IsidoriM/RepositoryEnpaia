@model TFI.OCM.Amministrativo.GestioneAziendeWebOCM
@{
	ViewBag.Title = "DettaglioAziendaWeb";
	Layout = "~/Views/Shared/_LayoutContainer.cshtml";
}

@{
	if (Model.datiAzienda == null)
	{
		Model.datiAzienda = new TFI.OCM.Amministrativo.GestioneAziendeWebOCM.DatiAzienda();
	}

	if (Model.altridati == null)
	{
		Model.altridati = new TFI.OCM.Amministrativo.GestioneAziendeWebOCM.AltriDati();
	}

	if (Model.sedeLegale == null)
	{
		Model.sedeLegale = new TFI.OCM.Amministrativo.GestioneAziendeWebOCM.SedeLegale();
	}

	if (Model.sedeAmministrativa == null)
	{
		Model.sedeAmministrativa = new TFI.OCM.Amministrativo.GestioneAziendeWebOCM.SedeAmministrativa();
	}

	if (Model.indirizzoCorrispondenza == null)
	{
		Model.indirizzoCorrispondenza = new TFI.OCM.Amministrativo.GestioneAziendeWebOCM.IndirizzoCorrispondenza();
	}

	if (Model.rapleg == null)
	{
		Model.rapleg = new TFI.OCM.Amministrativo.GestioneAziendeWebOCM.RapLeg();
	}

	if (Model.documenti == null)
	{
		Model.documenti = new TFI.OCM.Amministrativo.GestioneAziendeWebOCM.Documenti();
	}

}


@section showMessage{
	@{Html.RenderPartial("PartialViewShowMessages");}

